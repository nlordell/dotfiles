;;; -*- lexical-binding: t -*-

(defconst ligatures-berkeley-mono
  '(;; Group A
    ".." ".=" "..." "..<" "::" ":::" ":=" "::=" ";;" ";;;" "??" "???" ".?" "?."
    ":?" "?:" "?=" "**" "***" "/*" "*/" "/**"
    ;; Group B
    "<-" "->" "-<" ">-" "<--" "-->" "<<-" "->>" "-<<" ">>-" "<-<" ">->" "<-|"
    "|->" "-|" "|-" "||-" "<!--" "<#--" "<=" "=>" ">=" "<==" "==>" "<<=" "=>>"
    "=<<" ">>=" "<=<" ">=>" "<=|" "|=>" "<=>" "<==>" "||=" "|=" "//=" "/="
    ;; Group C
    "<<" ">>" "<<<" ">>>" "<>" "<$" "$>" "<$>" "<+" "+>" "<+>" "<:" ":<" "<:<"
    ">:" ":>" "<~" "~>" "<~>" "<<~" "<~~" "~~>" "~~" "<|" "|>" "<|>" "<||"
    "||>" "<|||" "|||>" "</" "/>" "</>" "<*" "*>" "<*>" ":?>"
    ;; Group D
    "#(" "#{" "#[" "]#" "#!" "#?" "#=" "#_" "#_(" "##" "###" "####"
    ;; Group E
    "[|" "|]" "[<" ">]" "{!!" "!!}" "{|" "|}" "{{" "}}" "{{--" "--}}" "{!--"
    "//" "///" "!!"
    ;; Group F
    "www" "@_" "&&" "&&&" "&=" "~@" "++" "+++" "/\\" "\\/" "_|_" "||"
    ;; Group G
    "=:" "=:=" "=!=" "==" "===" "=/=" "=~" "~-" "^=" "__" "!=" "!==" "-~" "--"
    "---")
  "Ligatures for the Berkeley Mono font.")
